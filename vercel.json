{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "npm install --legacy-peer-deps --force && npm install tsx --save-dev --legacy-peer-deps --force",
        "buildCommand": "npm run build"
      }
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
